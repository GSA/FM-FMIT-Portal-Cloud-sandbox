<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - FM IT Services Portal *** FOR U.S. GOVERNMENT USE ONLY ***</title>

    <environment include="Development">
        @* U.S. Web Design System 2.0 *@
        @* Visit https://v2.designsystem.digital.gov for U.S. Web Design System 2.0 user guide and document *@
        <link rel="stylesheet" href="~/uswds-2.0.0/css/uswds.css" />
        <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.2.0/css/all.css" integrity="sha384-hWVjflwFxL6sNzntih27bfxkr27PmbbK/iSvJ+a4+0owXq79v+lsFkW54bOGbiDQ" crossorigin="anonymous">
    </environment>
    <environment exclude="Development">
        @* Remove BootstrapCDN *@
        <link rel="stylesheet" href="~/uswds-2.0.0/css/uswds.min.css" asp-append-version="true" />
        <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.2.0/css/all.css" integrity="sha384-hWVjflwFxL6sNzntih27bfxkr27PmbbK/iSvJ+a4+0owXq79v+lsFkW54bOGbiDQ" crossorigin="anonymous">
    </environment>
    <link rel="stylesheet" href="~/css/site.min.css" asp-append-version="true" />
</head>
<body class="page-page-templates layout-styleguide">
    <a class="usa-skipnav" href="#main-content">Skip to main content</a>
    <section class="usa-banner">
        <div class="usa-accordion">
            <header class="usa-banner-header">
                <div class="usa-grid usa-banner-inner">
                    <img src="~/uswds-2.0.0/img/favicons/favicon-57.png" alt="U.S. flag">
                    <p>An official website of the United States government</p>
                    <button class="usa-accordion-button usa-banner-button"
                            aria-expanded="false" aria-controls="gov-banner">
                        <span class="usa-banner-button-text">Here's how you know</span>
                    </button>
                </div>
            </header>
            <div class="usa-banner-content usa-grid usa-accordion-content" id="gov-banner">
                <div class="usa-banner-guidance-gov usa-width-one-half">
                    <img class="usa-banner-icon usa-media_block-img" src="~/uswds-2.0.0/img/icon-dot-gov.svg" alt="Dot gov">
                    <div class="usa-media_block-body">
                        <p>
                            <strong>The .gov means it’s official.</strong>
                            <br>
                            Federal government websites often end in .gov or .mil. Before sharing sensitive information, make sure you’re on a federal government site.
                        </p>
                    </div>
                </div>
                <div class="usa-banner-guidance-ssl usa-width-one-half">
                    <img class="usa-banner-icon usa-media_block-img" src="~/uswds-2.0.0/img/icon-https.svg" alt="Https">
                    <div class="usa-media_block-body">
                        <p>
                            <strong>The site is secure.</strong>
                            <br>
                            The <strong>https://</strong> ensures that you are connecting to the official website and that any information you provide is encrypted and transmitted securely.
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </section>
    @*Body section*@
    <div class="usa-overlay"></div>
    <header class="usa-header usa-header-basic" role="banner">
        <div class="usa-nav-container">
            <div class="usa-navbar">
                <div class="usa-logo" id="basic-logo">
                    <img class="usa-header-slim-logo-img" src="~/images/gsa/nav-logo.jpg" alt="" hspace="5">
                    <em class="usa-logo-text"><a href="/" title="Home" aria-label="Home">FM IT Services</a></em>
                </div>
                <button class="usa-menu-btn">Menu</button>
            </div>

            @* Navigation bar in the header *@
            <nav role="navigation" class="usa-nav">
                <button class="usa-nav-close">
                    <img src="/uswds-2.0.0/img/close.svg" alt="close">
                </button>
                <ul class="usa-nav-primary usa-accordion">
                    <li>
                        <a class="usa-nav-link" href="https://@(ViewBag.hostnamePrefix)fmitservices.gsa.gov/arcs">
                            <span>ARCS</span>
                        </a>
                    </li>
                    <li>
                        <a class="usa-nav-link" href="https://@(ViewBag.hostnamePrefix)fmitservices.gsa.gov/cir">
                            <span>CIR</span>
                        </a>
                    </li>
                    @*<li>
            <a class="usa-nav-link" href="/Home/WIP">
                <span>IPAC</span>
            </a>
        </li>*@
                    <li>
                        <a class="usa-nav-link" href="https://@(ViewBag.hostnamePrefix)fmitservices.gsa.gov/nwtb">
                            <span>NWTB</span>
                        </a>
                    </li>
                    <li>
                        <a class="usa-nav-link" href="https://@(ViewBag.iisPrefix).ocfo.gsa.gov/VITAP">
                            <span>VITAP</span>
                        </a>
                    </li>
                    @*<li>
                        <a class="usa-nav-link" href="https://fedpay.gsa.gov">
                            <span>FEDPAY</span>
                        </a>
                    </li>*@
                    <li>
                        <a class="usa-nav-link" href="https://biedw.fas.gsa.gov/FPA/">
                            <span>FPA</span>
                        </a>
                    </li>
                    <li>
                        <a class="usa-nav-link" href="https://@(ViewBag.hostnamePrefix)ulo.gsa.gov/">
                            <span>ULO</span>
                        </a>
                    </li>
                    <li>
                        <button class="usa-accordion-button usa-nav-link" aria-expanded="false" aria-controls="basic-nav-section-one">
                            <span>Tools &amp; Reports</span>
                        </button>
                        <ul id="basic-nav-section-one" class="usa-nav-submenu">
                            @*<li>
            <a href="/Home/WIP">ARCS</a>
        </li>*@
                            <li>
                                <a href="https://@(ViewBag.iisPrefix).ocfo.gsa.gov/InvoiceSearch/">Invoice Search</a>
                            </li>
                            <li>
                                <a href="https://@(ViewBag.iisPrefix).ocfo.gsa.gov/paymentsearch/">Pegasys Payment Search</a>
                            </li>
                            <li>
                                <a class="usa-nav-link" href="https://@(ViewBag.hostnamePrefix)fmitservices.gsa.gov/rsnap">
                                    <span>RSNAP</span>
                                </a>
                            </li>
                            <li>
                                <a href="https://@(ViewBag.iisPrefix).ocfo.gsa.gov/worksheets/Msg.aspx">Worksheet Adjustments</a>
                            </li>
                            @*<li>
            <a href="/Home/WIP">WebBill</a>
        </li>*@
                        </ul>
                    </li>
                </ul>
                @*<form class="usa-search usa-search-small ">
                    <div role="search">
                        <label class="usa-sr-only" for="basic-search-field-small">Search small</label>
                        <input id="basic-search-field-small" type="search" name="search">
                        <button type="submit">
                            <span class="usa-sr-only">Search</span>
                        </button>
                    </div>
                </form>*@

            </nav>
        </div>
    </header>
    <div class="usa-overlay"></div>
    <main id="main-content"></main>


    @*<partial name="_CookieConsentPartial" />*@
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer class="usa-footer usa-footer-slim" role="contentinfo">
            @*<div class="usa-grid usa-footer-return-to-top">
                <a href="javascript:void(0);">Return to top</a>
            </div>*@
            <div class="usa-footer-primary-section">
                <div class="usa-grid">
                    <nav class="usa-footer-nav usa-width-two-thirds">
                        <ul class="usa-unstyled-list">
                            <li class="usa-width-one-half usa-footer-primary-content">
                                <a class="usa-footer-primary-link" href="https://www.gsa.gov/" target="_blank">GSA.gov</a>
                            </li>
                            <li class="usa-width-one-half usa-footer-primary-content">
                                <a class="usa-footer-primary-link" href="https://www.gsa.gov/about-us/organization/office-of-the-chief-information-officer/office-of-corporate-it-services" target="_blank">Corporate IT Services</a>
                            </li>
                        </ul>
                    </nav>
                    <div class="usa-width-one-third">
                        <ul class="usa-unstyled-list">
                            <li>
                                <address>
                                    <a class="usa-footer-primary-link" href="mailto:bdf.apps@gsa.gov">Email Us</a>
                                </address>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </footer>
    </div>
    <environment include="Development">
        <script src="~/lib/jquery/dist/jquery.js"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
        <script src="~/js/site.js" asp-append-version="true"></script>
        <script src="~/uswds-2.0.0/js/uswds.js"></script>
    </environment>
    <environment exclude="Development">
        <script src="https://ajax.aspnetcdn.com/ajax/jquery/jquery-3.3.1.min.js"
                asp-fallback-src="~/lib/jquery/dist/jquery.min.js"
                asp-fallback-test="window.jQuery"
                crossorigin="anonymous"
                integrity="sha384-tsQFqpEReu7ZLhBV2VZlAu7zcOV+rXbYlF2cqB8txI/8aZajjp4Bqd+V6D5IgvKT"></script>
        <script src="https://ajax.aspnetcdn.com/ajax/bootstrap/3.3.7/bootstrap.min.js"
                asp-fallback-src="~/lib/bootstrap/dist/js/bootstrap.min.js"
                asp-fallback-test="window.jQuery && window.jQuery.fn && window.jQuery.fn.modal"
                crossorigin="anonymous"
                integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa"></script>
        <script src="~/js/site.min.js" asp-append-version="true"></script>
        <script src="~/uswds-2.0.0/js/uswds.js"></script>
    </environment>
    @RenderSection("Scripts", required: false)
</body>
</html>
